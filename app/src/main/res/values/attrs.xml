<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Search_View">
        <attr name="textSizeSearch" format="dimension"  />
        <attr name="textColorSearch" format="color" />
        <attr name="textHintSearch" format="string" />
        <attr name="searchBlockHeight" format="integer" />
        <attr name="searchBlockColor" format="color" />
    </declare-styleable>

    <string-array name="print_page">
        <item>@string/stem</item>
        <item>@string/correct</item>
        <item>@string/incorrect</item>
        <item>@string/key</item>
        <item>@string/cause</item>
    </string-array>

    <string-array name="print_page_value">
        <item>0</item>
        <item>1</item>
        <item>2</item>
        <item>3</item>
        <item>4</item>
    </string-array>

    <string-array name="print_page_value_default">
        <item>0</item>
    </string-array>

    <string-array name="show_print">
        <item>@string/correct</item>
        <item>@string/incorrect</item>
        <item>@string/key</item>
        <item>@string/cause</item>
    </string-array>

    <string-array name="show_print_value">
        <item>0</item>
        <item>1</item>
        <item>2</item>
        <item>3</item>
    </string-array>

    <string-array name="view_smear_method">
        <item>@string/click_highlighter_by</item>
        <item>@string/click_button_by</item>
    </string-array>

    <string-array name="view_smear_method_value">
        <item>0</item>
        <item>1</item>
    </string-array>

</resources>